@model IEnumerable<PhoneBookMVC.Models.Contact>

@{
    ViewData["Title"] = "Contacts";
}
<table class="table">
    <tbody>
        @foreach (var item in Model) {
            <!-- Add Image? -->
            <tr>
                <td>
                    <a asp-action="Details" asp-route-id="@item.Id">
                        @Html.DisplayFor(modelItem => item.FullName)
                    </a>
                </td>
            </tr>
        }
    </tbody>
</table>
